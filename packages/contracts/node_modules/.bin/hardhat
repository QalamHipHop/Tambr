#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/internal/cli/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/internal/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/internal/cli/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/internal/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules/hardhat/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_b3684e825c29c3d66b8ccf1f80618158/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
