#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules/solidity-coverage/plugins/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules/solidity-coverage/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules/solidity-coverage/plugins/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules/solidity-coverage/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.27.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._7e3e57988d0aa3a5de8176b32702c9d1/node_modules:/home/ubuntu/Tambr/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
fi
